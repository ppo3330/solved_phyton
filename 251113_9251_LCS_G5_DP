#O(len(str1) * len(str2))
#DP를 이렇게 쓰네, 문자열 다루기 어렵다...
str1 = '0' + input()
str2 = '0' + input()

dp = [[0]* 1001 for _ in range(1001)]

for i in range(1,len(str1),1):
    for j in range(1,len(str2),1):
        if i == j and i == 0: continue
        elif str1[i] == str2[j]:
            dp[i][j] = dp[i-1][j-1]+1
        else:
            dp[i][j] = max(dp[i][j-1],dp[i-1][j])
            
print(dp[len(str1)-1][len(str2)-1])
